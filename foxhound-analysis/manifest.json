{
  "manifest_version": 2,
  "name": "Foxhound Analysis",
  "version": "1.0",

  "permissions": ["webRequest", "webNavigation", "*://*/*"],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["index.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
